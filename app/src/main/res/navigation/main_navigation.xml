<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/onborarding_navigation"
    app:startDestination="@id/onborardingFragment">

    <fragment
        android:id="@+id/onborardingFragment"
        android:name="com.bangkit.capstone.lukaku.ui.onboarding.OnborardingFragment"
        android:label="fragment_onborarding"
        tools:layout="@layout/fragment_onborarding">
        <action
            android:id="@+id/action_onborardingFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.bangkit.capstone.lukaku.ui.main.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />

</navigation>